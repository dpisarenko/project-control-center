<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
         "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
         "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="at.silverstrike.pcc.impl.persistence.DefaultBooking"
		table="TBL_BOOKING" dynamic-update="false" dynamic-insert="false">
		<id name="id" column="BOOKING_ID" type="java.lang.Long"
			unsaved-value="-1">
			<generator class="native">
			</generator>
		</id>
		<property name="startDateTime" />
		<property name="duration" />
 
		<many-to-one name="process"
			class="at.silverstrike.pcc.impl.persistence.DefaultControlProcess" />
		<many-to-one name="resource"
			class="at.silverstrike.pcc.impl.persistence.DefaultResource" />
	</class>
</hibernate-mapping>
